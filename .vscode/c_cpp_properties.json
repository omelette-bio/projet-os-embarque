{
    "version": 4,
    "configurations": [
        {
            "name": "AVR-ATmega328P",
            "compilerPath": "/usr/local/avr-gcc/bin/avr-g++",
            "intelliSenseMode": "gcc-x64",
            "cStandard": "gnu11",
            "cppStandard": "gnu++11",
            "defines": [
                "__AVR__",
                "__AVR_ATmega328P__",
                "F_CPU=16000000L",
                "ARDUINO=10812",
                "ARDUINO_AVR_UNO",
                "ARDUINO_ARCH_AVR"
            ],
            "compilerArgs": [
                "-mmcu=atmega328p"
            ],
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Arduino_FreeRTOS",
                "${workspaceFolder}/Arduino_FreeRTOS/FreeRTOS-Kernel/include",
                "${workspaceFolder}/Arduino_FreeRTOS/FreeRTOS-Kernel/portable/GCC/ATMega328",
                "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/cores/arduino",
                "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/libraries/Wire/src",
                "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/libraries/Wire/src/utility",
                "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/variants/standard",
                "/usr/local/avr-gcc/lib/gcc/avr/15.1.0/include",
                "/usr/local/avr-gcc/avr/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/Arduino_FreeRTOS",
                    "${workspaceFolder}/Arduino_FreeRTOS/FreeRTOS-Kernel/include",
                    "${workspaceFolder}/Arduino_FreeRTOS/FreeRTOS-Kernel/portable/GCC/ATMega328",
                    "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/cores/arduino",
                    "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/libraries/Wire/src",
                    "${workspaceFolder}/Arduino_FreeRTOS/arduinoLibsAndCore/variants/standard",
                    "/usr/local/avr-gcc/avr/include"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ]
}